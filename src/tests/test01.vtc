varnishtest "Test example vmod"

server s1 {
       rxreq
       txresp
} -start

varnish v1 -vcl+backend {
	import example from "${vmod_topbuild}/src/.libs/libvmod_substring.so";

	sub vcl_deliver {
	    if (substring.substr(req.url, "foo") {
		set resp.http.sub = "success";	
	    }
		
	}
} -start

client c1 {
	txreq -url "/foobar"
	rxresp
	expect resp.http.sub == "success"
}

client c1 -run
